{
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "routes": [
    {
      "route": "/a/hoge/index.html",
      "headers": {
        "Cache-Control": "no-cache, no-store"
      }
    },
    {
      "route": "/b/fuga/index.html",
      "headers": {
        "Cache-Control": "public, max-age=3600, immutable"
      }
    },
    {
      "route": "/_nuxt/*.{js,css}",
      "headers": {
        "Cache-Control": "public, max-age=3600, immutable"
      }
    }
  ]
}
